{
  "security": {
    "fetch_timeout_ms": 5000,
    "max_prompt_size": 102400,
    "request_rate_limit": 10,
    "memory_limit": "250MB",
    "allowed_prompt_sources": [
      "https://raw.githubusercontent.com/elder-plinius/L1B3RT4S/main",
      "https://elder-plinius.github.io"
    ]
  },
  "caching": {
    "enabled": true,
    "ttl_seconds": 86400,
    "max_cache_size_mb": 50
  },
  "logging": {
    "level": "INFO",
    "retention_days": 7,
    "metrics_interval_ms": 10000
  },
  "emergency_halts": {
    "consecutive_fetch_failures": 3,
    "memory_threshold_mb": 250,
    "cpu_threshold_percent": 90
  },
  "models": "auto"
}